<div class="row">
  <div class="center-block col-md-10">

    <% @results.each_with_index do |result, index| %>
      <div class="row">
        <div class="col-md-3">
          <%= index + 1 %>
        </div>
        <div class="col-md-3">
          <%= result.user.name %>
        </div>
        <div class="col-md-3">
          <%= result.creature.size %>
        </div>
        <div class="col-md-3">
          <%= result.location.name %>
        </div>
      </div>
    <% end %>

  </div>
</div>
